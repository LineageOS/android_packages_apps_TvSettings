<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/channels_and_inputs">
    <Preference
        android:key="channels"
        android:title="Channels"
        android:summary="Setup your TV to receive broadcast signal."/>
    <Preference
        android:key="preferences"
        android:title="Preferences(Channels)" />
    <Preference
        android:key="infoBanner"
        android:title="Info banner"
        android:summary="Display program information when channel is changed."/>
    <Preference
        android:key="parentalControls"
        android:title="Parental controls" />
    <Preference
        android:key="externalInputs"
        android:title="External inputs" />
    <Preference
        android:key="tvButtonsShortcut"
        android:title="TV buttons shortcut" />
</PreferenceScreen>
